<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Age_should_be_greater_than_18</fullName>
    <active>true</active>
    <errorConditionFormula>IF( (Age__c  &lt; 18),true,false)</errorConditionFormula>
    <errorMessage>Age must be greater than 18</errorMessage>
</ValidationRule>
