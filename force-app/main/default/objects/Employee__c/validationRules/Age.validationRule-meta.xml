<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Age</fullName>
    <active>true</active>
    <errorConditionFormula>IF( (Age__c   &lt;=  21)  ||  (Age__c   &gt;=  30),true,false)</errorConditionFormula>
    <errorMessage>Age must be between 21-30</errorMessage>
</ValidationRule>
