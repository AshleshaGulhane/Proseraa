<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Bank_Account_Number_cannot_be_changed</fullName>
    <active>true</active>
    <errorConditionFormula>AND(ISBLANK( Bank_Account_Number__c ) , ISCHANGED( Bank_Account_Number__c ) )</errorConditionFormula>
    <errorMessage>Bank Account number once set cannot be changed</errorMessage>
</ValidationRule>
